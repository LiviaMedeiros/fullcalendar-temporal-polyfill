{
  "name": "temporal-polyfill",
  "version": "0.0.0",
  "title": "Temporal Polyfill",
  "author": {
    "name": "Adam Shaw",
    "email": "arshaw@users.noreply.github.com",
    "url": "http://arshaw.com/"
  },
  "contributors": [
    {
      "name": "Ishaan Bharal",
      "email": "ishbharal@gmail.com"
    }
  ],
  "copyright": "2021 Adam Shaw",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./impl": {
      "require": "./dist/impl.cjs",
      "import": "./dist/impl.js"
    },
    "./shim": {
      "require": "./dist/shim.cjs",
      "import": "./dist/shim.js"
    },
    "./global": {
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    }
  },
  "files": [
    "/dist",
    "/*.d.ts"
  ],
  "scripts": {
    "e2e": "wtr --config wtr.config.cjs",
    "e2e-check": "tsc --project ./e2e"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@types/jest": "^26.0.24",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@web/dev-server-esbuild": "^0.2.14",
    "@web/test-runner": "^0.13.15",
    "jest": "^27.0.6",
    "jest-date-mock": "^1.0.8",
    "mocha": "^9.0.3",
    "typescript": "^4.3.5"
  }
}
