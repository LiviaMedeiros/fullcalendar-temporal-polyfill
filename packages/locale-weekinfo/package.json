{
  "name": "locale-weekinfo",
  "version": "0.0.0",
  "title": "Temporal Lite",
  "description": "A new JavaScript date library",
  "author": {
    "name": "Ishaan Bharal",
    "email": "ishbharal@gmail.com"
  },
  "contributors": [
    {
      "name": "Adam Shaw",
      "email": "arshaw@users.noreply.github.com",
      "url": "http://arshaw.com/"
    }
  ],
  "copyright": "2021 Adam Shaw",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "gzip:size": "gzip -c -r dist/ | wc -c | numfmt --to=iec-i --suffix=B",
    "build": "tsc && yarn esbuild && yarn esbuild:mini",
    "esbuild": "esbuild src/index.ts --bundle --outfile=dist/index.js",
    "esbuild:mini": "esbuild src/index.ts --bundle --minify --outfile=dist/index.min.js",
    "test": "jest",
    "write:firstDays": "node ./scripts/write-firstDays.mjs",
    "write:minimalDays": "node ./scripts/write-minimalDays.mjs",
    "write": "yarn write:firstDays && yarn write:minimalDays"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
